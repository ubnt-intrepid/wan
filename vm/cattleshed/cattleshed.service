[Unit]
Description=Backend of Sandbox Compiler

[Service]
Type=simple
User=cattleshed
Group=cattleshed
WorkingDirectory=/usr/local/cattleshed
Restart=on-failure
ExecStart=/usr/local/cattleshed/bin/cattleshed -c /usr/local/cattleshed/etc/cattleshed.conf -c /opt/wandbox/cattleshed-conf/compilers.default --syslog /var/log/cattleshed.log

[Install]
WantedBy=multi-user.target
